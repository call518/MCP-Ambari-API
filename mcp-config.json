{
  "mcpServers": {
    "ambar-api": {
      "command": "python",
      "args": ["-m", "mcp_ambari_api.ambari_api", "--log-level", "DEBUG"],
      "env": {
        "PYTHONPATH": "/app/src",
        "AMBARI_HOST": "host.docker.internal",
        "AMBARI_PORT": "8080",
        "AMBARI_USER": "admin",
        "AMBARI_PASS": "admin",
        "AMBARI_CLUSTER_NAME": "TEST-AMBARI"
      }
    }
  }
}
